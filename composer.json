{
    "name": "guysartorelli/cow-test-recipe",
    "type": "silverstripe-recipe",
    "description": "A recipe for testing silverstripe/cow",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GuySartorelli/cow-test-package"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/recipe-plugin": "^1",
        "guysartorelli/cow-test-package": "~1.1.0@rc"
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}